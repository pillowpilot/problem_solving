#include <iostream>
#include <vector>

using namespace std;

int f(int x, int y){
  if( x == 0 || y == 0 )
    return 1;
  else
    return f(x-1, y) + f(x, y-1);
}

int main(){

  for(int i = 0; i < 10; i ++){
    for(int j = 0; j < 10; j++ ){
      cout << f(i, j) << ' ';
    }
    cout << endl
  }
  
  return 0;
}
