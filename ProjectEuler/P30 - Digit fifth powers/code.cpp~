#include <iostream>

using namespace std;

inline bool isSolution(unsigned long long n, int power){
  unsigned long long sum = 0, t = n;
  while( n != ){
    int d = n % 10;
    for(int i = 1; i < power; i++)
      d *= d;
    sum += d;
    n /= 10;
  }
  return sum == t;
}

