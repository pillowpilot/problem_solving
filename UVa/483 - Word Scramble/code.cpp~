#include <cstdio>
#include <iostream>
#include <string>
#include <cstring>

using namespace std;

void reverseWord(char* word){
  char tmp;
  size_t len = strlen(word);
  
  for(size_t i = 0; i < len / 2; i++){
    tmp = word[i];
    word[i] = word[len - i];
    word[len - i] = tmp;
  }
}

