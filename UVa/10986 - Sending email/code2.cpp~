#include <cstdio>
#include <vector>
#include <queue>
#include <functional>

using namespace std;

typedef vector<int> vi;
typedef vector<vi> vvi;
typedef vector<bool> vb;

vvi weights;
vi distances;
vb marks;

int main(){
  int N;

  scanf("%d ", &N);
  for( int css = 1; css <= N; css++ ){
    int n, m, s, t;
    
    scanf("%d %d %d %d ", &n, &m, &s, &t);
    
    weights = vvi(n, vi() );
    distances = vi( n, numeric_limits<int>::max() );
    marks = vb( n, false );

    while( m-- > 0 ){
      int a, b, w;

      scanf("%d %d %d ", &a, &b, &w);
      
    }
  }
  return 0;
}
