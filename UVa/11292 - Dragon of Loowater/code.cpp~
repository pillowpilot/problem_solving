#include <cstdio>
#include <vector>

using namespace std;

int main(){
  int heads, knights;
  vector<int> hDia, kHei;

  do{
    scanf("%d %d", &heads, &knights);
    if( heads != 0 && knights != 0){
      hDia.resize(heads);
      vector<int>::iterator iter = hDia.begin();
      while(iter != hDia.end()){
	scanf("%d", &(*iter)):
	iter++;
      }
    }
  }while(heads != 0 && knights != 0);


  
  return 0;
}
