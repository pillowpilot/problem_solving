#include <cstdio>
#include <bitset>

using namespace std;

typedef bitset<11222> bs;

bs f1;
bs f2;
bs f3;

void readProblems(int friendId){
  int S;
  scanf("%d ", &S);
  while( S-- > 0 ){
    int p;
    scanf("%d ", &p);
    switch(friendId){
    case 1:
      f1.set(p);
      break;
    case 2:
      f2.set(p);
      break;
    case 3:
      f3.set(p);
      break;
    }
  }
}

int main(){
  int T;
  
  scanf("%d ", &T);
  for( int case = 1; case <= T; case++ ){
    int S;
    
  }
}
