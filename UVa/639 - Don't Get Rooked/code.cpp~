#include <cstdio>
#include <vector>

#define EMPTY 0
#define BLOCK 1

using namespace std;

typedef vector<int> vi;
typedef vector<vi> vvi;
typedef struct{

} position;

vvi board;

void bt(int x, int y){
  if( x == board.size() && y == board.size() ){
    
  }
}

void init(int dims){
  board = vvi( dims, vi( dims, EMPTY ) );
}

int main(){
  int dims;

  while( scanf("%d", &dims) == 1 ){
    init(dims);
    for(int i = 0; i < dims; i++){
      for(int j = 0; j < dims; j++){
	char c;
	scanf("%c", &c);
	if( c == 'X' ){
	  board[i][j] = BLOCK;
	}
      }
      scanf("%*c"); // Consume EOL
    }

    
  }
  return 0;
}
