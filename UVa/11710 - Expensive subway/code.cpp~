#include <iostream>
#include <map>
#include <vector>
#include <string>

using namespace std;

typedef unsigned long long ull;

struct edge{
  string a, b;
  ull cost;
  edge(string a, string b, ull cost): a(a), b(b), cost(cost) {}
  bool operator<(const edge& e){
    return cost < e.cost;
  }
};

struct vertex{
  vector<edge> ee;
  
};

int main(){
  
  return 0;
}
