#include <cstdio>
#include <map>

using namespace std;

int main(){
  int n;

  while( scanf("%d ", &n) == 1 ){
    while( n-- > 0 ){
      char country[81];
      char lover[81];
      scanf("%s ", country);
      scanf("%[^\n] ", lover);

      printf(">>%s\n", country);
      printf(">>%s\n", lover);
    }
  }
  return 0;
}
