#include <cstdio>
#include <math>

using namespace std;

bool noSolution;
int p, q, r, s, t, u;
double solution;

double eval(double x){
  return p * exp(-x) + q * sin(x) + r * cos(x) + s * tan(x) + t * x^2 + u;
}

void solve(){
  double l = 0, h = 1, eH = eval(h), eL = eval(l), eS;
  if( ( eL > 0 && eH > 0 ) || eL < 0 && eH < 0) ){
    noSolution = true;
  }else{
  int iterations = 0;
    solution = l + ((h-l) >> 1);
    eS = eval(solution);
    
    while( abs( eS ) > 1e-4 && iterations++ < 50){
      
    }
  }
}

int main(){
  while( scanf("%d %d %d %d %d %d", &p, &q, &r, &s, &t, &u) == 6 ){
    solve();
    
  }
  return 0;
}
