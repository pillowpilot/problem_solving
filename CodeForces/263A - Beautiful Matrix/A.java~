import java.util.Scanner;

public class B{
    public static void main(String[] args){
	Scanner in = new Scanner(System.in);

	int i = 1, j = 1;
	int x;
	do{
	    x = in.nextInt();
	    if( x == 0 ){
		if( j < 5 )
		    i++;
		else{
		    j++;
		    i = 1;
		}
	    }
	}while(x==0);

	System.out.println("i: " + i);
	System.out.println("j: " + j);
    }
}
