#include <iostream>
#include <vector>

using namespace std;

typedef vector<int> vi;
typedef vector<vi> vvi;

vvi g;
vi depth;

int main(){
  int n, k;

  cin >> n >> k;

  g = vvi( n, vi() );

  while( n-- > 0 ){
    int a, b;
    cin >> a >> b;
    g[a].push_back(b);
    g[b].push_back(a);
  }

  
  return 0;
}
