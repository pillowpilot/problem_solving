#include <iostream>
#include <vector>

using namespace std;

typedef vector<bool> vb;

void sieve(){
  vb nmbrs = vb(NMAX + 1, false);
  primes.push_back(2);
  primes.push_back(3);
  
  int k = 1;
  while( 6*k + 1 < nmbrs.size() ){
    if( !nmbrs[6*k-1] ){
      primes.push_back(6*k-1);
      for( int i = (6*k - 1)*(6*k - 1); i < nmbrs.size(); i += 6*k - 1 ) nmbrs[i] = true;
    }
    if( !nmbrs[6*k+1] ){
      primes.push_back(6*k+1);
      for( int i = (6*k + 1)*(6*k + 1); i < nmbrs.size(); i += 6*k + 1 ) nmbrs[i] = true;
    }
    k++;
  }
  
}

int main(){
  
  return 0;
}
