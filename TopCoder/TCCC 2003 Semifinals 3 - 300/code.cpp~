#include <vector>

using namespace std;

class ZigZag{
public:
  int longestZigZag(vector<int> sequence){
    vector<int> lengths = vector<int>(sequence.size(), 1);
    vector<bool> marks = vector<bool>(sequence.size(), false);
    
    vector<int>::const_iterator iter = sequence.begin();
    while( iter != sequence.end() ){
      
      iter++;
    }
  }
};
